{
  "name": "node-ncc-app-test",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "package": "run-s clean compile packing",
    "packing": "ncc build ./dist/main.js -o output/ -m -s",
    "compile": "tsc -p .",
    "clean": "rimraf dist output"
  },
  "devDependencies": {
    "@types/node": "18.11.15",
    "@vercel/ncc": "0.36.0",
    "rimraf": "4.1.2",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.2",
    "typescript": "4.9.4"
  }
}